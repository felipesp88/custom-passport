{
    "name": "z1lab/laravel-passport",
    "description": "Laravel Passport provides OpenID Connect server support to Laravel.",
    "keywords": ["laravel", "passport", "oauth", "openid"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/z1lab/laravel-passport/issues",
        "source": "https://github.com/z1lab/laravel-passport"
    },
    "authors": [
        {
            "name": "z1lab",
            "email": "webmaster@z1lab.com"
        },
        {
            "name": "Felipe Pinheiro",
            "email": "filipe-pinheiro@hotmail.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "firebase/php-jwt": "~3.0|~4.0|~5.0",
        "guzzlehttp/guzzle": "~6.0",
        "illuminate/auth": "~5.6",
        "illuminate/console": "~5.6",
        "illuminate/container": "~5.6",
        "illuminate/contracts": "~5.6",
        "illuminate/database": "~5.6",
        "illuminate/encryption": "~5.6",
        "illuminate/http": "~5.6",
        "illuminate/support": "~5.6",
        "league/oauth2-server": "^7.0",
        "phpseclib/phpseclib": "^2.0",
        "symfony/psr-http-message-bridge": "~1.0",
        "zendframework/zend-diactoros": "~1.0",
        "ext-json": "*"
    },
    "require-dev": {
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "Laravel\\Passport\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Laravel\\Passport\\PassportServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
